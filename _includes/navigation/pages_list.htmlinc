{% for accesskey in (1..10) %}
	{% for node in pages_list %}
	    {% if node.accesskey == accesskey and page.language == node.language %}
		    {% if group == null or group == node.group %}
		        {% if page.id == node.id %}
		            <li class="{{node.id}}">
		      	        <a href="{{node.url}}" class="active" accesskey="{{node.accesskey}}" 
		      	           title="{{node.title}}">{{node.title}}</a>
		            </li>
		         {% else %}
		            <li class="{{node.id}}">
		      	        <a href="{{node.url}}" accesskey="{{node.accesskey}}" 
		      	           title="{{node.title}}">{{node.title}}</a>
		            </li>
		        {% endif %}
		    {% endif %}
	    {% endif %}
	{% endfor %}
{% endfor %}
{% assign pages_list = nil %}
{% assign group = nil %}
