      {% assign pages_list = site.pages %}
      {% assign subgroup = 'ludzie' %}
      {% include navigation/ludzie_list.htmlinc %}        
    </ul>
{% endif %}
<h1>
    <a href="{{page.url}}" title="ars cantata">ars cantata</a>
</h1>
<div id="partners">
    {% if page.id == 'strona-glowna' %}
        <ul>
            <li>
                <a href="/en" title="Wersja angielska">
                    <img src="/img/lang-en.png" alt="Wersja angielska" />
                </a>
            </li>
        </ul>
    {% else %}
        <p>wsparcie:</p>
        <ul>
            <li>
                <a href="http://kik.waw.pl/" title="Klub Inteligencji Katolickiej">
                    <img src="/img/partners/kik.png" alt="Klub Inteligencji Katolickiej" />
                </a>
            </li>
            <li>
                <a href="http://mk.gov.pl/po/zagranica.php" title="Ministerstwo Kultury i Dzedzictwa Nardowego">
                    <img src="/img/partners/mk.png" alt="Ministerstwo Kultury i Dzedzictwa Nardowego" />
                </a>
            </li>
            <li>
                <a href="http://collegium.edu.pl/root_pl" title="Collegium Civitas">
                    <img src="/img/partners/cc.png" alt="Collegium Civitas" />
                </a>
            </li>
        </ul>
    {% endif %}
</div>

<div id ="face">
    {% if page.id != 'strona-glowna' %}
